<table>
  <thead>
    <tr>
      <th>Tender ID</th>
      <th>Date Modified</th>
    </tr>
  </thead>
  <tbody>
    <% @tenders.each do |t| %>
    <tr>
      <td><%= link_to t['id'], tenders_show_path(t['id']) %></td>
      <td><%= t['dateModified'] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
